{
  "version": 8,
  "configurePresets": [
    {
      "name": "dace",
      "hidden": false,
      "displayName": "DACE C++",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "installDir": "$env{HOME}/.local",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "/usr/bin/gcc",
        "CMAKE_CXX_COMPILER": "/usr/bin/g++",
        "CMAKE_CXX_STANDARD": "20",
        "CMAKE_INSTALL_PREFIX": "$env{HOME}/.local",
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "WITH_PTHREAD": "ON",
        "WITH_ALGEBRAICMATRIX": "ON",
        "WITH_EIGEN": "ON",
        "WITH_JULIA": "OFF"
      }
    },
    {
      "name": "julia",
      "hidden": false,
      "displayName": "DACE Julia",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "installDir": "$env{JULIA_DEPOT_PATH}/dev/DACE_jll/override",
      "cacheVariables": {
        "CMAKE_C_COMPILER": "/usr/bin/gcc",
        "CMAKE_CXX_COMPILER": "/usr/bin/g++",
        "CMAKE_CXX_STANDARD": "20",
        "CMAKE_INSTALL_PREFIX": "$env{JULIA_DEPOT_PATH}/dev/DACE_jll/override",
        "CMAKE_PREFIX_PATH": "$env{JULIA_DEPOT_PATH}/dev/libcxxwrap_julia_jll/override",
        "CMAKE_INSTALL_RPATH": "$env{JULIA_DEPOT_PATH}/dev/libcxxwrap_julia_jll/override/lib:$env{JULIA_DEPOT_PATH}/juliaup/julia-1.10.5+0.x64.linux.gnu/lib",
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "WITH_PTHREAD": "ON",
        "WITH_ALGEBRAICMATRIX": "ON",
        "WITH_EIGEN": "ON",
        "WITH_JULIA": "ON",
        "CUSTOM_EXIT": "ON"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "dace",
      "displayName": "DACE C++",
      "configurePreset": "dace"
    },
    {
      "name": "julia",
      "displayName": "DACE Julia",
      "configurePreset": "julia"
    }
  ]
}
